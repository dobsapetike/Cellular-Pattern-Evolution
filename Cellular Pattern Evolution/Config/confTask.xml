<Settings>
  <LatticeSettings>
    <LatticeWidth>10</LatticeWidth>
    <LatticeHeight>10</LatticeHeight>
    <CellType>Regular</CellType>
    <PhenotypeSettings>
      <NeighbourhoodType>VonNeumann</NeighbourhoodType>
    </PhenotypeSettings>
    <TargetPattern>Target/twoband90_10.target</TargetPattern>
    <InitPattern>
      TODO
    </InitPattern>
    <Controller>
      <Type>FeedForwardANNController</Type>
      <HiddenNeuronCount>1</HiddenNeuronCount>
      <ColorType>grayscale</ColorType>
    </Controller>
    <StopCriterion>
      <Criterion>EvalNums</Criterion>
      <Limit>100</Limit>
    </StopCriterion>
    <State>
      <InternalCount>2</InternalCount>
      <ExternalCount>2</ExternalCount>
      <InitialValues>0.66</InitialValues>
    </State>
  </LatticeSettings>

  <ObjectiveSettings>
    <MinValue>-1</MinValue>
    <MaxValue>1</MaxValue>
    <ObjectiveFunctions>
      <Function Name="ColorDist"/>
    </ObjectiveFunctions>
  </ObjectiveSettings>

  <OptimizerSettings>
    <Algorithm>MOCMAES</Algorithm>
    <Params>
      <InitStepSize>1.0</InitStepSize>
      <ParentSelectionCount>100</ParentSelectionCount>
      <EvolutionStepCount>5</EvolutionStepCount>
    </Params>
  </OptimizerSettings>
</Settings>
